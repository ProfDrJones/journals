<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>journals</id>
	<name>Journals</name>
	<summary>A Journal app for nextcloud</summary>
	<description><![CDATA[This app adds general vJournal support for Nextcloud and allows to create, edit and delete vJournal entries to the users calendars. It also implements an own category system in which journal entries can be grouped]]></description>
	<version>0.0.1</version>
	<licence>agpl</licence>
	<author mail="johannes@szeibert.de" homepage="http://jones.szeibert.de">Johannes Szeibert</author>
    <namespace>Journals</namespace>
	<documentation>
        <admin>https://github.com/ProfDrJones/journals#installation</admin>
        <developer>https://github.com/ProfDrJones/journals#developer-info</developer>
    </documentation>
    <category>productivity</category>
	<website>https://github.com/ProfDrJones/journals</website>
	<bugs>https://github.com/ProfDrJones/journals/issues</bugs>
    <repository>https://github.com/ProfDrJones/journals.git</repository>
    <screenshot></screenshot>
    <dependencies>
        <nextcloud min-version="16" max-version="19"/>
    </dependencies>
    <navigations>
        <navigation>
            <id>journals</id>
            <name>Journals</name>
            <route>journals.view.index</route>
            <icon>journals.svg</icon>
            <order>900</order>
        </navigation>
    </navigations>
</info>
